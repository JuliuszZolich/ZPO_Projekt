<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="477.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.projekt_zpo.Controllers.ConfirmDeleteStudentController">
   <children>
      <Label fx:id="ConfirmationLabel" layoutX="23.0" layoutY="35.0" text="Czy napewno chcesz usunąć studenta?">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="CancelDeleteConfirmation" layoutX="98.0" layoutY="133.0" mnemonicParsing="false" onMouseClicked="#cancel" prefHeight="26.0" prefWidth="86.0" text="Anuluj" />
      <Button fx:id="DeleteUserConfirmationButton" layoutX="283.0" layoutY="133.0" mnemonicParsing="false" onMouseClicked="#delete" prefHeight="26.0" prefWidth="86.0" text="Tak" />
   </children>
</AnchorPane>
