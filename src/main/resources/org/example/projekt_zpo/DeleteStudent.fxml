<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.projekt_zpo.Controllers.DeleteStudentController">
    <children>
        <Label alignment="CENTER" layoutX="75.0" layoutY="40.0" text="Usuń Studenta">
            <font>
                <Font name="System Bold" size="36.0" />
            </font>
        </Label>
        <Label layoutX="87.0" layoutY="111.0" prefHeight="18.0" prefWidth="166.0" text="Studenci:">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Label>
        <Button fx:id="CancelDeleteStudent" layoutX="87.0" layoutY="203.0" mnemonicParsing="false" onMouseClicked="#cancel" prefHeight="20.0" prefWidth="80.0" text="Anuluj" />
        <Button fx:id="DeleteStudent" layoutX="223.0" layoutY="203.0" mnemonicParsing="false" onMouseClicked="#delete" prefHeight="20.0" prefWidth="80.0" text="Usuń" />
        <Label fx:id="AddStudentErrorLabel" layoutX="164.0" layoutY="173.0" text="Błędny Index!" textFill="#c70505" underline="true" visible="false" />
        <ChoiceBox fx:id="choseStudent" layoutX="64.0" layoutY="138.0" prefHeight="26.0" prefWidth="270.0" value="Wybierz Studenta" />
        <Label fx:id="errorLabel" alignment="CENTER" layoutX="66.0" layoutY="173.0" prefHeight="18.0" prefWidth="270.0" text="Label" textFill="RED" visible="false">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
